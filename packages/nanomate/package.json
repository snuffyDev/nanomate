{
	"name": "@nanomate/core",
	"version": "0.0.1",
	"description": "Animation core for Nanomate",
	"author": "snuffyDev <snuffydev@hotmail.com>",
	"license": "MIT",
	"keywords": [
		"nanomate",
		"animation",
		"core",
		"web animation",
		"animation library",
		"waapi",
		"web animation api",
		"motionpath",
		"motion path",
		"animate"
	],
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"build": "tsup src/index.ts",
		"lint": "eslint src --ext .ts",
		"lint:fix": "eslint src --ext .ts --fix",
		"format": "prettier --write src/**/*.{ts,js}",
		"format:check": "prettier --check src/**/*.{ts,js}"
	},
	"devDependencies": {
		"tsup": "^7.2.0"
	}
}
